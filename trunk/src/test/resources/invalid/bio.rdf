  <rdf:Description rdf:about="http://purl.org/vocab/bio/0.1/">
    <skos:changeNote rdf:nodeID="A47"/>
    <vann:termGroup rdf:resource="http://purl.org/vocab/bio/0.1/termgroup5"/>
    <vann:termGroup rdf:resource="http://purl.org/vocab/bio/0.1/termgroup2"/>
    <vann:example rdf:resource="http://purl.org/vocab/bio/examples/1"/>
