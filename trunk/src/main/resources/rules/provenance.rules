[
  (?entity http://xmlns.com/foaf/0.1/isPrimaryTopicOf ?url  )
  (?entity p:CHAN ?c)
  (?c p:DATE ?d)
  (?d p:TIME ?t)
  (?d rdfs:label ?date)
  (?t rdfs:label ?time)
  strConcat(?date,?time,?dt)
->
  (?url http://purl.org/dc/terms/modified ?dt)
]
[
  (?entity http://xmlns.com/foaf/0.1/isPrimaryTopicOf ?url  )
  (?subm rdf:type t:SUBM)
->
  (?url http://purl.org/dc/terms/publisher ?subm)
  (?url http://purl.org/dc/terms/license http://creativecommons.org/licenses/by-nc/3.0/)
]