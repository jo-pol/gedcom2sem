# SELECT distinct ?s ?p ?o WHERE { ?s ?p "http://sws.geonames.org/2743477/about.rdf"}

SELECT DISTINCT 
  ?lat
  ?long
  str(?lat)
  str(?long)
{ SERVICE <http://factforge.net/sparql>
  { <http://sws.geonames.org/2743856/>
      wgs84_pos:lat ?lat;
      wgs84_pos:lat ?long.
  }
}
